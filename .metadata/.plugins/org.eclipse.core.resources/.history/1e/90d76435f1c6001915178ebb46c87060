
public class Initials {
	
	public static void main(String[] args) {
		
		System.out.println();
        System.out.println("            GGGGG            BBB               RRR");
        System.out.println("           GGG               B   B             R   R");
        System.out.println("          GGG                B     B           R     R");
        System.out.println("         GGG                 B      B          R      R");
        System.out.println("        GGGG   GGG           B     B           R     R");
        System.out.println("         GGG     G           BBBB B            R R R");
        System.out.println("          GGG    G           B      B          R  R");
        System.out.println("           GGG   G           B     B           R   R");
        System.out.println("            GGG  G           B   B             R    R");
        System.out.println("             GGGGG           BBB               R     R");
        System.out.println();
		
	}

}
